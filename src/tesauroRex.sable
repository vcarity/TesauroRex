Package tesauroRex;

Helpers

lowercase  = ['a' .. 'z'];
uppercase  = ['A' .. 'Z'];
underscore = '_';

letter     = lowercase | uppercase;

Tokens

identifier = letter+ ( letter+ | underscore )+;